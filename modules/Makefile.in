@SET_MAKE@
SUBDIR=ldap base64 zlib example
RECURSIVE_MAKE=@RECURSIVE_MAKE@
SHELL = /bin/sh
RM = rm -f

all:
	-for d in $(SUBDIR) ; do (cd ./$${d} && $(RECURSIVE_MAKE) $@) ; done

install clean mostlyclean:
	-for d in $(SUBDIR) ; do (cd ./$${d} && $(RECURSIVE_MAKE) $@) ; done

distclean:
	$(RM) Makefile config.*
	-for d in $(SUBDIR) ; do (cd ./$${d} && $(RECURSIVE_MAKE) $@) ; done

realclean extraclean:
	$(RM) *~ \#*
	-for d in $(SUBDIR) ; do (cd ./$${d} && $(RECURSIVE_MAKE) $@) ; done
